{
  "name": "valley-tpl",
  "version": "1.0.5",
  "description": "valley tpl",
  "main": "dist/valleymodule-tpl.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "rollup": "node_modules/.bin/rollup",
    "build-single": "rollup src/index.js --format umd --name \"vtpl\" --output dist/valleytpl.js",
    "build": "rollup --config rollup.node.config.js",
    "build-web": "rollup -c rollup.config.js",
    "build-node": "rollup src/output/node.js --format cjs --name \"Valley\" --output dist/vtpl-node.js",
    "build-plus": "rollup src/output/index-plus.js --format umd --name \"vtpl\" --output dist/valleytpl.js",
    "build-plus-node": "rollup src/output/node-plus.js --format cjs --name \"vtpl\" --output dist/vtpl-node.js",
    "build-koa2": "rollup src/output/koa2-view.js --format cjs --name \"vtpl\" --output dist/vtpl-koa2.js",
    "build-koa": "rollup src/output/koa-view.js --format cjs --name \"vtpl\" --output dist/vtpl-koa.js",
    "build-express": "rollup src/output/express-view.js --format cjs --name \"vtpl\" --output dist/vtpl-express.js",
    "build-vm": "rollup src/output/vm-view.js --format umd --name RenderModule --output dist/valleymodule-tpl.js",
    "node": "rollup src/tpl-node.js -c rollup.config.js",
    "dev": "node_modules/.bin/rollup -c",
    "demo": "nodemon --exec node_modules/.bin/babel-node demo.js",
    "test": "jest",
    "test_karma": "karma",
    "test_jasmine": "jasmine JASMINE_CONFIG_PATH=test/unit/jasmine.json"
  },
  "jest": {
    "transform": {
      ".jsx?$": "babel-jest"
    },
    "testMatch": [
      "**/__tests__/**/*spec.js?(x)"
    ]
  },
  "repository": {
    "type": "git",
    "url": "gito:valley-tpl.git"
  },
  "keywords": [],
  "author": "valley",
  "email": "hit.gutianyu@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.0.4",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "jest": "^22.0.4",
    "rollup": "^0.53.1",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-uglify": "^2.0.1"
  },
  "dependencies": {
    "valley-module": "^1.0.5"
  }
}
