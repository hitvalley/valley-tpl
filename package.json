{
  "name": "valley_tpl",
  "version": "1.0.0",
  "description": "valley tpl",
  "main": "tpl-forvalleyjs.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "rollup": "node_modules/.bin/rollup",
    "build": "rollup src/index.js --format umd --name \"vtpl\" --output dist/valleytpl.js",
    "build-node": "rollup src/node.js --format cjs --name \"Valley\" --output dist/vtpl-node.js",
    "build-plus": "rollup src/index-plus.js --format umd --name \"vtpl\" --output dist/valleytpl.js",
    "build-plus-node": "rollup src/node-plus.js --format cjs --name \"vtpl\" --output dist/vtpl-node.js",
    "node": "rollup src/tpl-node.js -c rollup.config.js",
    "dev": "node_modules/.bin/rollup -c",
    "demo": "nodemon --exec node_modules/.bin/babel-node demo.js",
    "test": "jasmine JASMINE_CONFIG_PATH=test/unit/jasmine.json",
    "test_karma": "karma",
    "test_jasmine": "jasmine JASMINE_CONFIG_PATH=test/unit/jasmine.json"
  },
  "repository": {
    "type": "git",
    "url": "gito:valley_tpl.git"
  },
  "keywords": [],
  "author": "valley",
  "email": "hit.gutianyu@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "jasmine": "^2.8.0",
    "jasmine-node": "^1.14.5",
    "karma": "^1.7.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.0",
    "lodash.clonedeep": "^4.5.0",
    "mocha": "^3.5.0",
    "rollup-plugin-json": "^2.3.0"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "rollup": "^0.43.1"
  }
}
